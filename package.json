{
  "name": "github-vscode-icons",
  "version": "1.0.9",
  "description": "show vscode-icons in github repository",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "script:make-zip": "ts-node ./scripts/make-dist-zip.script.ts",
    "build:dev": "npm run test && npx webpack --config webpack.config.dev.js && npm run script:make-zip",
    "build:prod": "npx webpack --config webpack.config.prod.js && npm run script:make-zip",
    "lint": "npm run lint:packages && npm run lint:json && npm run lint:typings",
    "lint:packages": "prettier --write ./packages/**/*.{ts,d.ts,tsx}",
    "lint:json": "prettier --parser json --write ./*.json",
    "lint:typings": "prettier --write ./typings/**/*.d.ts",
    "dev": "npx webpack --watch --config webpack.config.dev.js",
    "dev:prod": "npx webpack --watch --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dderevjanik/github-vscode-icons"
  },
  "author": {
    "email": "daniel.derevjanik@gmail.com",
    "name": "Daniel Derevjanik"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": false,
    "testEnvironment": "node"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dderevjanik/github-vscode-icons/issues"
  },
  "homepage": "https://github.com/dderevjanik/github-vscode-icons#readme",
  "dependencies": {
    "@types/archiver": "^2.1.0",
    "@types/chalk": "^0.4.31",
    "@types/chrome": "0.0.49",
    "@types/jest": "^21.1.10",
    "@types/node": "^8.9.5",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "fastdom": "^1.0.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "select-dom": "^4.1.1",
    "vscode-icons-js": "^1.1.1"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "chalk": "^2.3.2",
    "jest": "^21.2.1",
    "node-fetch": "^1.7.3",
    "prettier": "^1.11.1",
    "ts-jest": "^21.2.4",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.11.0"
  }
}
